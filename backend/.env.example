# Google OAuth 2.0 Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# JWT Configuration
SECRET_KEY=your_jwt_secret_key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=your_token_expiry_in_minutes
SECURE=<False for development and True for production>

# Application Configuration
FRONTEND_URL=http://localhost:your_frontend_port
BACKEND_URL=http://localhost:your_backend_port

# Environment
ENVIRONMENT=<development or production>

#Database
DATABASE_URL=your_database_url
APP_VERSION=your_app_version

#Qdrant
QDRANT_URL=your_qdrant_db_url
QDRANT_API_KEY=your_qdrant_api_key
QDRANT_COLLECTION_NAME=your_jobs_collection_name
EMBEDDING_MODEL=your_embedding_model

#Groq API Key
GROQ_API_KEY=your_groq_api_key

LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langchain_api_key
LANGCHAIN_PROJECT=your_langchain_project

GMAIL_APP_PASSWORD=your_gmail_app_password

LLAMA_CLOUD_API_KEY=your_llama_cloud_api_key

# Timezone Configuration
TIMEZONE=your_preferred_timezone e.g. Asia/Kathmandu
